{
  "Logging": {
    "Console": {
      "IncludeScopes": true
    },
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "EventLog": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "ElasticSearchStore": {
    "EndpointAddress": "http://localhost:9200",
    "IndexName": "hwinfo",
    "TimeoutMs": "1000",
    "MaxRetries": "2"
  },
  "BackgroundWorkers": {
    "StoreHwInfoElementWorker": {
      "DelayMs": 10000
    }
  }
}